library ieee;
use ieee.std_logic_1164.all;
use ieee.std_logic_arith.all;
use ieee.std_logic_unsigned.all;

entity ControleULA is
	port(
		OpULA: in std_logic_vector( 1 downto 0 ); --	O que recebe do controle geral
		funct: in std_logic_vector( 5 downto 0 ); --	Campo funtion da instrucao
		oper: out std_logic_vector( 2 downto 0 )  --	O que a ula vai fazer
	);
end entity;
